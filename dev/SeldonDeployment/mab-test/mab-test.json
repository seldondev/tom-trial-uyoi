{
    "kind": "SeldonDeployment",
    "apiVersion": "machinelearning.seldon.io/v1alpha2",
    "metadata": {
        "name": "mab-test",
        "namespace": "dev",
        "creationTimestamp": null,
        "labels": {
            "fluentd": "true"
        }
    },
    "spec": {
        "name": "mab-test",
        "predictors": [
            {
                "name": "default",
                "graph": {
                    "name": "epsilon-greedy-router",
                    "children": [
                        {
                            "name": "mock-classifier-a",
                            "type": "MODEL"
                        },
                        {
                            "name": "mock-classifier-b",
                            "type": "MODEL"
                        }
                    ],
                    "type": "ROUTER",
                    "parameters": [
                        {
                            "name": "n_branches",
                            "value": "2",
                            "type": "INT"
                        },
                        {
                            "name": "epsilon",
                            "value": "0.2",
                            "type": "FLOAT"
                        },
                        {
                            "name": "verbose",
                            "value": "1",
                            "type": "BOOL"
                        }
                    ]
                },
                "componentSpecs": [
                    {
                        "metadata": {
                            "creationTimestamp": null
                        },
                        "spec": {
                            "containers": [
                                {
                                    "name": "mock-classifier-a",
                                    "image": "seldonio/mock_classifier:1.6.0-dev",
                                    "resources": {
                                        "limits": {
                                            "cpu": "1",
                                            "memory": "1Gi"
                                        },
                                        "requests": {
                                            "cpu": "1",
                                            "memory": "1Gi"
                                        }
                                    },
                                    "imagePullPolicy": "IfNotPresent"
                                }
                            ],
                            "terminationGracePeriodSeconds": 20
                        }
                    },
                    {
                        "metadata": {
                            "creationTimestamp": null
                        },
                        "spec": {
                            "containers": [
                                {
                                    "name": "mock-classifier-b",
                                    "image": "seldonio/mock_classifier:1.6.0-dev",
                                    "resources": {
                                        "limits": {
                                            "cpu": "1",
                                            "memory": "1Gi"
                                        },
                                        "requests": {
                                            "cpu": "1",
                                            "memory": "1Gi"
                                        }
                                    },
                                    "imagePullPolicy": "IfNotPresent"
                                }
                            ],
                            "terminationGracePeriodSeconds": 20
                        }
                    },
                    {
                        "metadata": {
                            "creationTimestamp": null
                        },
                        "spec": {
                            "containers": [
                                {
                                    "name": "epsilon-greedy-router",
                                    "image": "seldonio/mab_epsilon_greedy:1.6.0-dev",
                                    "resources": {}
                                }
                            ],
                            "terminationGracePeriodSeconds": 20
                        }
                    }
                ],
                "replicas": 1,
                "engineResources": {},
                "svcOrchSpec": {
                    "env": [
                        {
                            "name": "SELDON_ENABLE_ROUTING_INJECTION",
                            "value": "true"
                        }
                    ]
                }
            }
        ]
    },
    "status": {}
}