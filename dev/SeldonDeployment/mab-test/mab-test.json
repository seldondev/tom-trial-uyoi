{
    "kind": "SeldonDeployment",
    "apiVersion": "machinelearning.seldon.io/v1",
    "metadata": {
        "name": "mab-test",
        "namespace": "dev",
        "creationTimestamp": null,
        "labels": {
            "app": "seldon"
        }
    },
    "spec": {
        "name": "poc-eg",
        "predictors": [
            {
                "name": "eg-2",
                "graph": {
                    "name": "eg-router",
                    "children": [
                        {
                            "name": "rf-model",
                            "type": "MODEL"
                        },
                        {
                            "name": "xgb-model",
                            "type": "MODEL"
                        }
                    ],
                    "type": "ROUTER",
                    "parameters": [
                        {
                            "name": "n_branches",
                            "value": "2",
                            "type": "INT"
                        },
                        {
                            "name": "epsilon",
                            "value": "0.1",
                            "type": "FLOAT"
                        },
                        {
                            "name": "verbose",
                            "value": "1",
                            "type": "BOOL"
                        },
                        {
                            "name": "branch_names",
                            "value": "rf:xgb",
                            "type": "STRING"
                        },
                        {
                            "name": "seed",
                            "value": "1",
                            "type": "INT"
                        }
                    ]
                },
                "componentSpecs": [
                    {
                        "metadata": {
                            "creationTimestamp": null
                        },
                        "spec": {
                            "containers": [
                                {
                                    "name": "rf-model",
                                    "image": "seldonio/credit_default_rf_model:0.2",
                                    "resources": {}
                                },
                                {
                                    "name": "xgb-model",
                                    "image": "seldonio/credit_default_xgb_model:0.2",
                                    "resources": {}
                                },
                                {
                                    "name": "eg-router",
                                    "image": "seldonio/mab_epsilon_greedy:1.6.0-dev",
                                    "resources": {}
                                }
                            ],
                            "terminationGracePeriodSeconds": 20
                        }
                    }
                ],
                "replicas": 1,
                "annotations": {
                    "predictor_version": "v1"
                },
                "engineResources": {},
                "svcOrchSpec": {
                    "env": [
                        {
                            "name": "SELDON_ENABLE_ROUTING_INJECTION",
                            "value": "true"
                        }
                    ]
                }
            }
        ],
        "annotations": {
            "deployment_version": "v1",
            "project_name": "Epsilon Greedy Routing"
        }
    },
    "status": {}
}